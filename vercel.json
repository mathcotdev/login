{
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"views/**/*.hbs",
            "use":"@vercel/static-build"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/index.js"
        }
    ],
    "env":{
        "NODE_ENV": "production"
    }
}